         <pb facs="uva.00070.001" xml:id="leaf01" type="recto"/>
         <lg
              >

		

            <head type="main" rend="underline">A <subst>
                  <rdg varSeq="1">
                     <del rend="overwrite">of</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="over">
                        <gap reason="scrawled fragment"/>
                     </add>
                  </rdg>
               </subst>Thought of <add rend="unmarked" place="supralinear">
                  <gap reason="scrawled fragment"/>
               </add>Columbus</head>
 
            <note type="authorial"
                  place="right">brought to 1891</note>

            <note type="authorial"
                  place="right">
               <gap reason="scrawled fragment"/>
            </note>

            <gap reason="scrawled, heavily overstruck"/>


            <l>The mystery of mysteries, <addSpan rend="unmarked" to="a1" place="right"/>the <add rend="insertion" place="supralinear">crude and</add>hurried <add rend="insertion" place="supralinear">ceaseless</add>flame,<anchor xml:id="a1"/>
               <add rend="unmarked" place="infralinear">spontaneous bearing on itself<unclear reason="crowded type, scrawled" resp="#mm"
                           cert="high">!</unclear>
               </add>
            </l>

            <l>The bubble<unclear reason="scrawled" resp="#mm" cert="medium">s</unclear> and the <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">
                        <unclear reason="heavily overstruck" resp="#mm"
                                 cert="90%">great</unclear>
                     </del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">huge</add>
                  </rdg>
               </subst>
               <unclear reason="scrawled" resp="#mm" cert="medium">hu</unclear>
               <gap reason="scrawled"/> 
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">world</del>!</rdg>
                  <rdg varSeq="2">
                     <add rend="insertion" place="supralinear">concrete orb</add>
                  </rdg>
               </subst>
            </l>

            <l>
               A breath of Deity, as <add rend="insertion" place="supralinear">
                     <del rend="overstrike">bulging</del>
                  </add> 
                  <unclear reason="scrawled, misspelled" resp="#mm"
                           cert="medium">thense</unclear>
                  <del rend="overstrike">as</del>
                  <add rend="insertion" place="supralinear">the bulging</add> universe<lb/>
               unfolding!<lb/>
            </l>

            <l>
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">
                        <gap reason="scrawled"/> of years</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">
                        <del rend="overstrike">Cycles</del>
                     </add>
                  </rdg>
                  <rdg varSeq="3">
                     <add rend="unmarked" place="supralinear">The many issuing cycles</add>
                  </rdg>
               </subst>
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">and</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">from</add>
                  </rdg>
               </subst> their preced<subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">
                        <unclear reason="overstruck, scrawled, misspelled" resp="#mm"
                                 cert="medium">ory</unclear>
                     </del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">ent</add>
                  </rdg>
               </subst> minutes!</l>

            <l>
               The <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">cycles</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">eras</add>
                     </rdg>
                  </subst> of the soul <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">inspiring</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">incepting</add>
                     </rdg>
                  </subst> 
                  <del rend="overstrike">in</del> an<lb/>
               hour<lb/>
            </l>

            <l>
               
                  <unclear reason="scrawled, misspelled" resp="#mm"
                           cert="medium">Hapaly</unclear> the widest farthest <unclear reason="scrawled, misspelled" resp="#mm"
                           cert="medium">evolutioning</unclear>
               <lb/>
               of the world and man.<lb/>
            </l>

            <note type="authorial"
                  place="right">
               <gap reason="deletion, illegible"/>
            </note>

