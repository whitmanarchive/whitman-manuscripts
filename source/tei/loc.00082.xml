    <text type="manuscript">
        <body>
            <pb facs="loc.00082.001.jpg" xml:id="leaf001r" type="recto"/>
            <lg type="poem">
                <l><del rend="overstrike">Rivulets,</del> some <subst>
                            
                                <del rend="overstrike" seq="1">meandering</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">threading</add>
                                                    </subst>
<subst>
                            
                                <del rend="overstrike" seq="1">through</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">Ohio's</add>
                                                    </subst>

                    <lb/>
<del rend="overstrike">the</del>
                        <del rend="overstrike">fields</del> farm-fields, or
                    <lb/>
<del rend="overstrike">in</del> the woods
                </l>
                <l>Some <del rend="overstrike">flowing</del> down <del rend="overstrike">fr</del>
                        <del rend="overstrike">as from</del>

                    <lb/>Colorado's <subst>
                            
                                <del rend="overstrike" seq="1">mountains</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">ca&#241;ons</add></subst>, <subst>
                            
                                <del rend="overstrike" seq="1">their</del>
                                                        
                                <del rend="overstrike" seq="3"><add place="supralinear" rend="unmarked" seq="2">from</add></del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="4">from</add>
                                                    </subst> sources
                    <lb/>
<subst>
                            
                                <del rend="overstrike" seq="1">the</del>
                                                        
                                <del rend="overstrike" seq="3"><add place="supralinear" rend="unmarked" seq="2">of</add></del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="3">of</add>
                                                    </subst> eternal snow,
                </l>
                <metamark function="transpose" xml:id="tr01" target="#tr02"/>
                <l>Some <add place="supralinear" rend="unmarked">to the north</add> finding
                        their way to <del rend="overstrike">Michigan,</del>

                    <lb/>Erie, Niagara, Ottawa,
                </l>
                <l>Some to <del rend="overstrike">the bays</del> Atlantics bays,
                    <lb/>and so to the great salt <subst>
                            
                                <del rend="overstrike" seq="1">sea</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">brine</add></subst>.
                </l>
                <transpose anchored="1" xml:id="tr02" target="#tr01"><add rend="unmarked" place="bottom"><l>Some <subst>
                        
                        <del rend="overstrike" seq="1">to</del>
                        
                        <del rend="overstrike" seq="2">with</del>
                        
                        <del rend="overstrike" seq="4"><add place="supralinear" rend="unmarked" seq="3">in their</add></del>
                    </subst> half&#8209;hid <del rend="overstrike">threads</del> in
                        Oregon,&#8212;or
                        <lb/>southward in Texas,
                    
                </l></add></transpose>
            </lg>
            <pb facs="loc.00082.002.jpg" xml:id="leaf001v" type="verso"/>
        </body>
