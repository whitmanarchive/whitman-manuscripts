    <text type="manuscript">
        <body>
            <pb facs="loc.00213.001.jpg" xml:id="leaf001r" type="recto"/>
            <lg type="poem">
                <note type="authorial" place="top">sent H May 19</note>
                <note type="authorial" place="top">pub'd</note>
                <head type="main" rend="underline">Life and Death</head>
                <l>The two old <add rend="insertion" place="supralinear">simple</add> problems <add rend="unmarked" place="inline">ever</add>
</l>
                <l>
<del rend="overstrike">For</del>
                    <del rend="overstrike">
<subst>
                            
                                <del rend="overwrite" seq="1">e</del><add place="over" rend="overwrite" seq="2">E</add></subst>ver</del> intertwined</l>
                <metamark function="transpose" xml:id="tr01"/>
                <l>

<subst>
                            
                                <del rend="overstrike" seq="1">For</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">By</add>
                                                    </subst>
<del rend="overstrike">
                            <gap reason="deleted, illegible" extent="about two letters"/>
                        </del> each successive age <del rend="overstrike">pass'd on</del>
<lb/>
                    insoluble, pass'd on,
                </l>
                <l>To ours <choice>
<orig>to-day</orig><reg>today</reg></choice>—and we pass on. <add rend="unmarked" place="inline">the same</add>
</l>
                <transpose anchored="1" target="#tr01">
                    <l>
                        <add rend="insertion" place="supralinear">Close home,</add> Elusive, <del rend="overstrike">simple,</del> present, <add rend="unmarked" place="supralinear">baffled,</add> grappled, <subst>
                            
                            <del rend="overstrike" seq="1">foil'd</del>
                            
                            <del rend="overstrike" seq="3">
                                <add place="supralinear" rend="unmarked" seq="2">baffled,</add>
                            </del>
                        </subst>
                    </l>
                </transpose>
            </lg>
            <pb facs="loc.00213.002.jpg" xml:id="leaf001v" type="verso"/>
            <note type="editorial" resp="#h1" place="top">ORIGINAL MANUSCRIPT OF A POEM BY WALT WHITMAN</note> 
        </body>
    </text>
