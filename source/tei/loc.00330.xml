         <pb facs="loc.00330.001" xml:id="leaf001r" type="recto"/>

         <lg
              >

            <head type="main-derived" rend="bracketed">Deep mystery of mysteries!</head>

            <note type="authorial"
                  place="top"
                  rend="right">4</note>

            <l>Deep mystery of mysteries!</l>

            <l>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">Mapping all</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">
                           <add rend="insertion" place="top" rend="left">Hinting and</add> 
                           <note type="authorial" place="inline">?</note> Holding somehow</add>
                     </rdg>
                  </subst> 
                  <del rend="overstrike">thou orb,</del> 
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">and all</del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">whole</add>
                        </del>
                     </rdg>
                     <rdg varSeq="3">
                        <add rend="unmarked" place="supralinear">all</add>
                     </rdg>
                  </subst> we<lb/>
               are and all we see, and <del rend="overstrike">all</del> the <add rend="insertion" place="supralinear">whole</add>
               <lb/>
               
                  <add rend="unmarked" place="supralinear">manifold</add> orb—creation<lb/>
            </l>

            <l>And its preceding thought—and</l>
            <!--WW CONVERTS LINE BELOW INTO A SEGMENT BY DELETING "AND"-->
            <l>
               <del rend="overstrike">And</del> the great vast ineffable known</l>
            <l>And <add rend="unmarked" place="supralinear">the</add> far <del rend="overstrike">more</del> greater, <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">greater</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">vast</add>
                  </rdg>
               </subst> unknown,</l>

            <note type="authorial"
                  place="bottom"
                  rend="bracketed">4</note>

            <pb facs="loc.00330.002" xml:id="leaf001v" type="verso"/>


         </lg>
