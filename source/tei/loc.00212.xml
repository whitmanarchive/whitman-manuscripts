      <body>
         <pb facs="loc.00212.001" xml:id="leaf001r" type="recto"/>
         <lg
              >
            <head type="main-authorial" rend="underline">
               <subst>
                  <rdg varSeq="1">The last</rdg>
                  <rdg varSeq="2">
                     <add place="supralinear" rend="unmarked">
                        <note type="authorial" place="inline">?</note> Last words</add>
                  </rdg>
               </subst>
            </head>
            <l>
               
                  <add place="supralinear" rend="unmarked">To yet</add> Anothe<subst>
                     <rdg varSeq="1">
                        <del rend="overwrite">rs</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add place="over" rend="overwrite">r</add>
                     </rdg>
                  </subst> lease, (doubtless the last,)<lb/>
            </l>
            <l>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">Subjoining</del> 
                     </rdg>
                     <rdg varSeq="2">
                        <add place="supralinear" rend="unmarked">
                           <del rend="overstrike">
                              <note type="authorial" place="inline">?</note>
                           </del> Subscribing</add> 
                     </rdg>
                  </subst> still the same old strain,<lb/>
               
                  <add place="supralinear" rend="insertion">tallying</add> a<subst>
                     <rdg varSeq="1">
                        <del rend="overwrite">n</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add place="over" rend="overwrite">t</add>
                     </rdg>
                  </subst> end as at beginning—<lb/>
            </l>
            <add rend="insertion" place="bottom">
               <l>By <add place="supralinear" rend="insertion">head and</add> heart of me—by voice and pen—wafted to you</l>
               <l>To you <add place="supralinear" rend="insertion">whoever</add> 
                  <del rend="overstrike">now</del> reading—<subst>
                     <rdg varSeq="2">
                        <add place="supralinear" rend="insertion">—to you</add> 
                     </rdg>
                     <rdg varSeq="1">
                        <del rend="overstrike">you</del> 
                     </rdg>
                  </subst>America</l>
            </add>
            <del rend="overstrike">
               <l>
                  As behind <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">eac</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add place="supralinear" rend="unmarked">all</add>
                           </del> 
                        </rdg>
                     </subst>and each foregoing piece<lb/>
                  and page<lb/>
               </l>
            </del>
            <l>
               My <del rend="overstrike">beating</del> heart and hand, <add place="supralinear" rend="unmarked">
                     <note type="authorial" place="inline">?</note> America,</add> my voice<lb/>
            </l>
            <l>
               <add place="supralinear" rend="insertion">Union,</add> Equality and Love</l>
            <pb facs="loc.00212.002" xml:id="leaf001v" type="verso"/>
         </lg>
