                        </del>
                     </rdg>
                  </subst> 
                  <del rend="overstrike">the savage and</del> steady<add rend="unmarked" place="supralinear">the</add> roar of the<lb/>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">waving</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="interlinear">gale,</add>
                     </rdg>
                  </subst>
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">
