                        </del>
                     </rdg>
                     <rdg varSeq="3">
                        <add rend="unmarked" place="interlinear">with</add>
                     </rdg>
                  </subst> incessant<add rend="insertion" place="interlinear">undertow</add> muttering<lb/>
            </l>

            <l>
               <del rend="overstrike">B</del>
               <delSpan rend="overstrike"
                        to="d1"
                        />
               <add rend="unmarked" place="supralinear">The</add>Fitful<anchor xml:id="d1"/>
