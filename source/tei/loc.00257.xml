                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <gap reason="deletion, illegible"/>
                        </del>
                     </rdg>
                  </subst>
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">the black night</del>
                     </rdg>
                     <rdg varSeq="2">
