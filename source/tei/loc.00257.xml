               Sea
<del rend="overstrike">
                     <add rend="unmarked" place="supralinear">wind</add>
                  </del> confronting,<lb/>
            </l>
            <anchor xml:id="a3"/>

            <pb facs="loc.00257.002" xml:id="leaf001v" type="verso"/>

            <addSpan type="pasteon" place="over" to="a4"/>
            <note type="authorial"
