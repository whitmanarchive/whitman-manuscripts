                        <rdg varSeq="1">
                           <gap reason="deletion, illegible"/>
                        </rdg>
                        <rdg varSeq="2">
                           <add rend="unmarked" place="interlinear">Your slaves</add>
                        </rdg>
                     </subst> to fury lasting!<lb/>
               </l>
            </del>
            <l>
               
                  <subst>
                     <rdg varSeq="1">Through</rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="interlinear">Where in</add>
                     </rdg>
                  </subst>
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">the</del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="interlinear">midn</add>
                        </del>
                     </rdg>
                  </subst> 
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">midnights</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="interlinear">thickest</add>
                     </rdg>
                  </subst>murk, through the<lb/>
               
