                  place="left">TR</note>
            <transpose rend="bracketed" anchored="no">
               <l>
                  Out<subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">there</del>
                        </rdg>
                        <rdg varSeq="2">
                           <add rend="unmarked" place="supralinear">in</add>
                        </rdg>
                     </subst>the darkness<del rend="overstrike">
                        <add rend="unmarked" place="supralinear">midnight</add>
                     </del>there the<lb/>
