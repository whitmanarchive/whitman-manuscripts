                     <rdg varSeq="3">
                        <add rend="unmarked" place="interlinear">S<del rend="overstrike">orest</del>
                           <add rend="unmarked" place="interlinear">ends</add> winds whistling</add>
                     </rdg>
                  </subst>
                  <!---TRANSCRIBED THIS WAY TO INDICATE THAT THE "S" IS USED FOR BOTH WORDS, BUT CANNOT NEST <subst>S WITHIN EACH OTHER.-->
