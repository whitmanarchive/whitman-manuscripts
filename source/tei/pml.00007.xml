    <text type="manuscript">
        <body>
            <pb facs="pml.00007.001.jpg" xml:id="leaf001r" type="recto"/>
            <lg type="poem">
                <head type="main" rend="underline">Starry Union</head>
                <l>See! See! see! 
                        <seg xml:id="alt01">how</seg>                         
                            <add place="supralinear" rend="unmarked" xml:id="alt02">where</add><alt target="#alt01 #alt02" weights="0.5 0.5"/>
                    
                    the sun is 
                    <subst>
                        <del rend="overstrike" seq="1">blazing</del>
                                                
                            <del rend="overstrike" seq="3">
                                <add place="supralinear" rend="unmarked" seq="2">streaming</add>
                            </del>
                                                
                            <add place="supralinear" rend="unmarked" seq="4">beaming</add></subst>!</l>
                <l>See! See! see! all the bright stars <subst>
                        
                            <del rend="overstrike" seq="1">shining</del>
                                                
                            <add place="supralinear" rend="unmarked" seq="2">gleaming</add></subst>!</l>
                <l>
                    North, or South, <subst>
                            
                                <del rend="overstrike" seq="1">or Westward</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">in order</add>
                                                    </subst>
<subst>
                            
                                <del rend="overstrike" seq="1">beaming</del>
                                                        
                                <del rend="overstrike" seq="3">
                                    <add place="supralinear" rend="unmarked" seq="2">moving</add>
                                </del>
                                                        
                                <add rend="unmarked" place="infralinear" seq="4"><unclear reason="illegible" resp="#nhg" cert="medium">loving</unclear></add>
                                                    </subst>

                    <add place="interlinear" rend="unmarked">
                        <lb/>all <del rend="overstrike">Equal all</del> Including, folding,
                    </add>
                </l>
                <l>Union all! O its Union all!</l>
                <l>O its all for <subst>
                        
                            <del rend="overstrike" seq="1">one</del>
                                                
                            <add place="supralinear" rend="unmarked" seq="2">each</add></subst>, &amp; <subst>
                        
                            <del rend="overstrike" seq="1">one</del>
                                                
                            <add place="supralinear" rend="unmarked" seq="2">each</add>
                                            </subst> for all!</l>
            </lg>
            <pb facs="pml.00007.002.jpg" xml:id="leaf001v" type="verso"/>
            <note type="archival" place="left" resp="#h1">MA 578(4)</note>
        </body>
    </text>
