         <pb facs="nyp_ch.00010_large.jpg" xml:id="leaf001r" type="recto"/> 
			
			<lg type="linegroup">
			   
			   <note type="authorial" place="left">?</note><span rend="bracket-left" xml:id="s1" from="#s1" to="#s2"/>
			   <l>I ask nobody's faith.... I am very little concerned about that</l>
			   
			   <l><subst><del rend="overstrike" seq="1">When</del> <del rend="overstrike" seq="3"><add rend="unmarked" place="supralinear" seq="2">Do</add></del></subst> <subst><del rend="overwrite" seq="1">y</del><add rend="overwrite" place="over" seq="2">Y</add></subst>ou doubt <add rend="insertion" place="supralinear">not</add> the east and the west.</l>
			   
			   <l><subst><del rend="overstrike" seq="1">When</del> <del rend="overstrike" seq="3"><add rend="unmarked" place="supralinear" seq="2">Do</add></del></subst> <subst><del rend="overwrite" seq="1">y</del><add rend="overwrite" place="over" seq="2">Y</add></subst>ou doubt <add rend="insertion" place="supralinear">not</add> your desires <subst><del rend="overstrike" seq="1">and</del> <add rend="unmarked" place="supralinear" seq="2">or</add></subst> your finger&#8209;nails,<!-- close up symbol used here --></l>
			
         
			   <l><subst><del rend="overstrike" seq="1">When</del> <del rend="overstrike" seq="3"><add rend="unmarked" place="supralinear" seq="2">Do</add></del></subst> <subst><del rend="overwrite" seq="1">y</del><add rend="overwrite" place="over" seq="2">Y</add></subst>ou doubt <add rend="unmarked" place="supralinear">not</add> <subst><del rend="overstrike" seq="1">iron</del> <del rend="overstrike" seq="3"><add rend="unmarked" place="supralinear" seq="2">water</add></del> <add rend="unmarked" place="supralinear" seq="4">metal</add></subst> or acid or steam</l><anchor xml:id="s2"/>
         
         
         
         
           
