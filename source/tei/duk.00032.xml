        <body>
            <pb facs="duk_nhg.00406_large.jpg" xml:id="leaf001r" type="recto"/>
            <lg type="poem">
                <delSpan rend="hashmark" spanTo="#d1"/>
                <l xml:id="l01">
                   
<subst>
                            
                                <del rend="overstrike" seq="1">Great are you,</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">You</add>
                                                    </subst>lusty and <subst>
                            
                                <del rend="overstrike" seq="1">loving</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">
                                    <choice>
<sic>graceflu</sic>
<corr>graceful</corr>
</choice>
                                </add>
                                                    </subst>youth! <del rend="overstrike">but</del>

                    <lb/>you are great
                    <lb/>
<add place="inline" rend="unmarked">you</add> are not exclusively great<del rend="overstrike">,</del>
                        <add place="supralinear" rend="unmarked">in youth.—</add>
                        <del rend="overstrike">graceful</del>

                    <lb/>
                        <del rend="overstrike">and fascinating youth;</del>
                    
                </l>
                <l>
<add place="supralinear" rend="unmarked">Your</add> Middle age<subst>
                            
                                <del rend="overstrike" seq="1"><unclear reason="illegible" cert="medium" resp="#hm">comes</unclear></del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">shall be great</add>
                                                    </subst> with <del rend="overstrike">its</del> amplitude
                    <lb/>and <del rend="overstrike"><gap reason="illegible" extent="about three letters"/></del> steadiness and <del rend="overstrike">ma</del>

                    <lb/>
<subst>
                            
                                <del rend="overstrike" seq="1">allmastering</del>
                                                        
                                <del rend="overstrike" seq="3">
                                    <add place="supralinear" rend="unmarked" seq="2">the</add>
                                </del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="4">fullblooded</add>
                                                    </subst> strength
                </l>
                <l xml:id="l02">Your old age <subst>
                            
                                <del rend="overstrike" seq="1">
                                    <unclear reason="illegible" cert="medium" resp="#hm">come
                                        with</unclear>
                                </del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">shall be equally
                                    great</add>
                                                    </subst>
<subst>
                            
                                <del rend="overstrike" seq="1">shall have</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2">with</add>
                                                    </subst>

                    <lb/>
<del rend="overstrike">resistless</del> majesty and bloom and
                    <lb/>fascination and love,
                </l>
                <anchor xml:id="d1"/>
            </lg>
