    <text type="manuscript">
        <body>
            <pb facs="loc.00006.001.jpg" xml:id="leaf001r" type="recto"/>
            <lg type="poem">
                <head type="main-authorial">America</head>
                <lg type="linegroup">
                    <l>No Homer, <choice>
<orig>Shakspere</orig><reg>Shakespeare</reg></choice>, Voltaire </l>
                    <l>No <del rend="overstrike">palaces,</del> Kings' palaces <add place="supralinear" rend="unmarked">n</add>or courts, </l>
                    <l>No<add place="supralinear" rend="unmarked">r </add>armies, <subst>
                            
                                <del rend="overstrike" seq="1">navies</del>
                                                        
                                <add place="supralinear" rend="unmarked" seq="2"> on the </add>
                                                    </subst> land, nor navies on the sea, </l>
                </lg>
                <lg type="linegroup">
                    <l>
<add place="supralinear" rend="unmarked">But </add> countless living equal
                        men</l>
                    <l>Average free </l>
                </lg>
            </lg>
            <pb facs="loc.00006.002.jpg" xml:id="leaf001v" type="verso"/>
        </body>
    </text>
