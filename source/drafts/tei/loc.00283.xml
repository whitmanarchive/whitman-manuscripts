         <pb facs="loc.00283.001" xml:id="leaf001r" type="recto"/>

         <lg
              >

            <head type="main-derived">
Utter Prostrate
</head>

            <l>
               <add rend="unmarked" place="left">Utter</add>Prostrate, with face bent <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">to</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">in</add>
                  </rdg>
               </subst> the sands,</l>

            <l>
               <del rend="overstrike">O God</del> Thee, thee O God, I praise <del rend="overstrike">to</del> thee <add rend="unmarked" place="infralinear">supplicate</add>
            </l>

            <l>Here on the sea-shore, <del rend="overstrike">sands,</del>
            </l>

            <l>Kneeling, O God, <add rend="insertion" place="supralinear">bereft of all,</add> utterly prostrate, <del rend="overstrike">of all bereft,</del>
            </l>

            <l>
               <hi rend="underline">Deus creavit</hi>, with face bent in the sands,</l>

            <l>To Thee, then, <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">I <add rend="unmarked" place="supralinear">
                           <unclear reason="deletion, illegible"
                                    cert="90%" resp="#fl">alone</unclear>
                        </add>
                     </del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="inline">I <add rend="unmarked" place="supralinear">still</add>
                     </add>
                  </rdg>
               </subst> 
make <del rend="overstrike">my</del> report,</l>

         </lg>

