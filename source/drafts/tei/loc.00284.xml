      <body>

         <pb facs="loc.00284.001" xml:id="leaf001r" type="recto"/>

         <lg
              >


            <head type="main-derived">
The terminus now near
</head>

            <l>The terminus now <subst>
                  <rdg varSeq="1">near</rdg>
                  <rdg varSeq="2">
                     <del rend="overstrike">
                        <add rend="unmarked" place="supralinear">close</add>
                     </del>
                  </rdg>
               </subst>
            </l>
            <subst>
               <rdg varSeq="1">
                  <del rend="overstrike">
                     <l>And near the</l>
                  </del>
               </rdg>
               <rdg varSeq="2">
                  <del rend="overstrike">
                     <l>But open</l>
                  </del>
               </rdg>
            </subst>
            <l>My <subst>
                  <rdg varSeq="1">end</rdg>
                  <rdg varSeq="2">
                     <del rend="erasure">
                        <add rend="unmarked" place="interlinear">termin</add>
                     </del>
                  </rdg>
                  <rdg varSeq="3">
                     <add rend="unmarked" place="interlinear">terminus</add>
                  </rdg>
               </subst> 
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">draws</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="interlinear">is</add>
                  </rdg>
               </subst> 
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">near</del>
                  </rdg>
                  <rdg varSeq="2">
                     <del rend="overstrike">
                        <add rend="unmarked" place="interlinear">at hand</add>
                     </del>
                  </rdg>
                  <rdg varSeq="3">
                     <add rend="unmarked" place="interlinear">near</add>
                  </rdg>
               </subst>,</l>
            <l>
               <delSpan rend="overstrike"
                        to="d1"
                        />My <gap reason="deletion, illegible"/> hands, my <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">
                        <gap reason="deletion, illegible"/>
                     </del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="interlinear">limbs</add>
                  </rdg>
               </subst> grow<add rend="unmarked" place="inline">n</add> nerveless<anchor xml:id="d1"/>
            </l>
            <l>
               The clouds <add rend="insertion" place="inline">are</add> 
                  <subst>
                     <rdg varSeq="1">closing</rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="interlinear">
                           <gap reason="illegible"/>ing</add>
                     </rdg>
                  </subst> in upon me—<del rend="overstrike">I</del>
               <lb/>
               
                  <del rend="overstrike"><!--WORD--></del> rejected <add rend="unmarked" place="interlinear">
                     <gap reason="illegible"/>
                  </add> 
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">
                           <gap reason="deletion, illegible"/>
                        </del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="interlinear">dying</add>
                     </rdg>
                  </subst>
               <lb/>
            </l>
            <l>
               My voyage <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">yet undone</del>—</rdg>
                     <rdg varSeq="2">
                        <add rend="insertion" place="interlinear">but commenced—already</add>
                     </rdg>
                  </subst>lost,<lb/>
               
                  <del rend="overstrike">balk'd,</del> 
                  <del rend="overstrike">disputed</del>, <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">balk'd.</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="insertion" place="interlinear">yet balk'd—</add>
                     </rdg>
                  </subst>
               <lb/>
            </l>
            <l>I yield my ships to Thee.—<del rend="overstrike">wes</del>
            </l>



         </lg>
