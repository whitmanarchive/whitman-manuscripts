<del rend="overwrite" seq="1">b</del>
<add place="over" rend="overwrite" seq="1">B</add></subst>reaks <subst>
<del rend="overstrike" seq="1">the</del> <add place="supralinear" rend="insertion" seq="2">
<del rend="overstrike">forth</del> yet the</add> </subst>lightning<del rend="overstrike">'s</del> 
