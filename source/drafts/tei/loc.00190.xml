            <!--  EDIT THE VALUE OF @FACS TO CORRESPOND WITH THE NAME OF THE RECTO IMAGE  -->
            <pb xml:id="leaf001r" facs="duk_nhg.00755_large.jpg" type="recto"/>
            <lg type="poem">
                <l><del rend="overstrike" seq="1">Covering</del> <add rend="insertion" place="above">Lay on</add> the graves of all dead soldiers</l>
                <l>Wreaths of roses and branches of palms,</l>
                <l>Not for the dead alone&#8212;for</l>
                <l><del rend="overstrike">Thee</del>meanings, indications</l>
                <l><del rend="overstrike">With quell'd and</del></l>
                <l>All wars, contnetions past</l>
                <l><del rend="overstrike">Tokens</del> <hi rend="underline">Decoration</hi> Day</l>
                <l><add rend="insertion" place="above">tokens</add>reciprical over the north and over the south</l>
                <l>One day <add rend="insertion" place="above">at least</add> hot passions laid&#8212;peace, <add rend="insertion" place="above" seq="1">[gap]</add> <del rend="overstrike" seq="2">[gap]</del> <add rend="insertion" place="above">uprisen</add> brotherhood</l>
                <l>We jining proud, remembering thankul <add rend="insertion" place="below">sorrows</add></l>
                <l>[gap]</l>
            </lg>
            
            <!--  EDIT THE VALUE OF THE @FACS TO CORRESPOND WITH THE NAME OF THE VERSO IMAGE  -->
