         <pb facs="uva.00268.001.jpg" xml:id="leaf001r" type="recto"/>
         <note type="editorial" resp="#h1" place="right"><del rend="erasure">18</del></note>
         <lg type="linegroup">

            <l>
               <addSpan rend="insertion" place="bottom" to="a1"/>The mystic
<subst>
                  <rdg varSeq="1">
                     <del rend="overstrike"> loot</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">roll</add>
                  </rdg>
               </subst>
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike"> of</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">from</add>
                  </rdg>
               </subst> all America<anchor xml:id="a1"/>
            </l>

            <l> 
               From wooded Maine,<del rend="overstrike"> and many a</del>
               <lb/>
               New England's farms,<del rend="overstrike">and farm</del>
               <lb/>
               fertile Pennsylvania,<lb/>
            </l>

            <l>
               The boundless West, the South,<add rend="insertion" place="supralinear">the Carolinas,</add> Texas,<lb/>
            </l>

         </lg>
