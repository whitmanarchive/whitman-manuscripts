   <pb xml:id="leaf001r" facs="10.1a%202.1%20Now%20Precedent%20Leaf%201%20Front.jpg" type="recto"/>
   
   <head rend="underline">Now precedent songs, Farewell</head>
   <lg type="poem">
     <l><add hand="#h02" place="inline">Now precedent songs, farewell&#8212;by <add place="below"><metamark function="add" target="#a001">&#8248;</metamark></add><add place="above" xml:id="a001">every</add> name farewell,</add></l>
    <milestone unit="glued"/>
     <l>(Trains <subst><del seq="1" rend="overstrike">like</del><add seq="2" place="above">of</add></subst> a staggering line<lb/>
        in many a strange procession<add hand="#h02" place="below"><metamark function="add" target="#a002">&#8248;</metamark></add><add hand="#h02" place="above" xml:id="a002">waggons,</add></l>
     <l><subst hand="#h02"><del seq="1" rend="overstrike">Of</del><add seq="2" place="above">From</add></subst> ups and downs&#8212;<subst hand="#h02"><del seq="1" rend="overstrike">of</del><add seq="2" place="above">with</add></subst>intervals&#8212;<subst hand="#h02"><del seq="1" rend="overstrike">of</del><add seq="2" place="above">from</add></subst> elder<lb/>
        years, mid-age, or youth,)</l>
    <milestone unit="glued"/>
     <l>"In cabin'd ships," or <add hand="#h02" place="inline">"</add>Thee Old Cause<add hand="#h02" place="inline">"</add> or<lb/>
        <del rend="overstrike">calling</del> "Poets to Come"</l>
     <l>Or "Paumanok," <del rend="overstrike">or</del> "Song of Myself<add hand="#h02" place="inline">,</add>"<lb/>
        <del rend="overstrike">or</del> "Calamus<add hand="#h02" place="inline">,</add>" or "Adam<add hand="#h02" place="inline">,</add>"</l>
     <l>Or "Beat! Beat! Drums!" or "to the Leaven'd<lb/>
        Soil they Trod,"</l>
     <l>Or "Captain! My Captain!" "Kosmos,"<lb/>
        "Quicksand Years" or "Thoughts."</l>
   </lg>
   
