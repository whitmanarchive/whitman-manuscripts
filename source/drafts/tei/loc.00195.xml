   <text>
      <body>
         <pb facs="loc.00195.001" xml:id="leaf001r" type="recto"/>
         <lg>
            <head type="main-authorial">
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">Swelling and ebbing and hand in hand</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add place="supralinear" rend="unmarked">
                        <del rend="overstrike">Advancing, retreating,</del>
                     </add>
                  </rdg>
                  <rdg varSeq="3">
                     <add place="inline" rend="unmarked">Ebb and Flood Tides.</add>
                  </rdg>
               </subst>
            </head>
            <l>
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">Swelling and ebbing</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add place="supralinear" rend="unmarked">Advancing, retreating,</add>
                  </rdg>
               </subst> and hand in hand</l>
            <l>
               <add place="supralinear" rend="insertion">Duly</add> With <add place="supralinear" rend="insertion">
                  <del rend="hashmark">their</del>
               </add> 
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">rhythmic</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add place="supralinear" rend="unmarked">turning</add>
                  </rdg>
               </subst> steps <add place="supralinear" rend="insertion">and mouths</add> to the <subst>
                  <rdg varSeq="1">
                     <del rend="hashmark">left</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add place="supralinear" rend="unmarked">right</add>
                  </rdg>
               </subst> or <add place="supralinear" rend="unmarked">the</add> 
               <subst>
                  <rdg varSeq="1">
                     <del rend="hashmark">right</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add place="supralinear" rend="unmarked">left</add>
                  </rdg>
               </subst>
            </l>
            <l>
               <gap reason="cut away"/> 
               <add place="supralinear" rend="unmarked">
                  <del rend="overstrike">reverting</del>
               </add> 
               <subst>
                  <rdg varSeq="1">
                     <add place="supralinear" rend="unmarked">
                        <del rend="overstrike">
                           <unclear reason="deletion, illegible"
                                    cert="high" resp="#fl">moving</unclear>
                        </del>
                     </add>
                  </rdg>
                  <rdg varSeq="2">
                     <add place="supralinear" rend="unmarked">reverting</add>
                  </rdg>
               </subst>
            </l>
            <l>
               <del rend="overstrike">With many a laugh</del>
            </l>
            <l>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">The</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add place="supralinear" rend="unmarked">You</add>
                     </rdg>
                  </subst> 
                  <del rend="overstrike">tides the</del> 
                  <del rend="overstrike">ceaseless</del> 
                  <add place="supralinear" rend="unmarked">
                     <delSpan spanTo="#d1"
                              rend="overstrike"
                              />liquid</add> tides like a <anchor xml:id="d1"/> chorus<lb/>
               of brothers and sisters<lb/>
            </l>
            <l>
               <del rend="overstrike">Move forever, <add place="supralinear" rend="unmarked">move,</add> round the circles of the earth,</del>
            </l>
            <l>Moving forever round the circles of the earth.</l>
         </lg>
      </body>
