   <text>
      <body>
         <pb facs="yal.00011.001" xml:id="leaf001r" type="recto"/>
         <note place="top" resp="#unk"
               type="editorial">HG 196</note>
         <note place="left" resp="#unk"
               type="editorial">523</note>
         <delSpan spanTo="#d1" rend="hashmark" />
         <lg
              >
            <head rend="underline" type="main-authorial">Rise, lurid stars.</head>
            <lg type="linegroup"
                 >
               <l>Rise, lurid stars, woolly white no more;</l>
               <l>
                  Change, angry cloth—weft of the silver stars<lb/>
                  no more;<lb/>
               </l>
               <l>
                  
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">Stars</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <add place="supralinear" rend="unmarked">Orbs</add> 
                        </rdg>
                     </subst>blushing scarlet—thirty four stars, red<lb/>
                  as flame,<lb/>
               </l>
               <l>On the blue bunting this day we sew.</l>
            </lg>
            <lg type="linegroup"
                 >
               <l>
                  World take good notice, silver stars have<lb/>
                  <milestone unit="glued"/>
                  vanished;<lb/>
               </l>
               <l>
                  Orbs <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">of the naked sword</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <add place="supralinear" rend="unmarked">now of scarlet</add>
                        </rdg>
                     </subst>—<del rend="overstrike">
                        <add place="supralinear" rend="unmarked">now</add>
                     </del> 
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">baleful</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <restore rend="dotted" desc="stet" type="del" TEIform="restore">
                              <del rend="overstrike">
                                 <add place="supralinear" rend="unmarked">mortal</add>
                              </del>
                           </restore> 
                        </rdg>
                     </subst>coals, all<lb/>
                  aglow,<lb/>
               </l>
               <l>Dots of molten iron, <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">hissing</del> 
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add place="supralinear" rend="unmarked">
                              <unclear reason="deletion, illegible" resp="#bb"
                                       cert="medium">quenc</unclear>
                           </add>
                        </del> 
                     </rdg>
                  </subst>wakeful and ominous,</l>
               <l>On the blue bunting <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">henceforth</del> 
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add place="supralinear" rend="unmarked">plainly</add>
                        </del> 
                     </rdg>
                     <rdg varSeq="3">
                        <add place="supralinear" rend="insertion">henceforth</add> 
                     </rdg>
                  </subst>appear</l>
            </lg>
         </lg>
         <anchor xml:id="d1"/>
         <note place="bottom" resp="#unk"
               type="editorial">H.G. 196</note>
         <pb facs="yal.00011.002" xml:id="leaf001v" type="verso"/>
