      <body>

		
         <pb facs="yal.00042.001" xml:id="leaf001r" type="recto"/>


         <lg>

            <head type="main-derived">
Such, such
</head>

            <note type="authorial"
                  place="left">TR</note>
            <anchor xml:id="t1"/>
            <del rend="overstrike">
               <l>To the <subst>
                     <rdg varSeq="1">last of the tide</rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">waning day and the last of the</add>
                     </rdg>
                  </subst> tide outrunning</l>
            </del>
            <l>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">What</del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">The</add>
                        </del>
                     </rdg>
                     <rdg varSeq="3">
                        <add rend="unmarked" place="interlinear">Such, such</add>
                     </rdg>
                  </subst> 
                  <del rend="overstrike">dreams</del> ye <add rend="insertion" place="supralinear">
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">seem to</del>
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add rend="insertion" place="supralinear">seem to</add>
                           </del>
                        </rdg>
                     </subst>
                  </add> quench <add rend="insertion" place="supralinear">and drown</add> 
                  <del rend="overstrike">ye</del>
                  <add rend="unmarked" place="supralinear">remorseless</add> 
                  <del rend="overstrike">streams</del>
               <lb/>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">
                           <gap reason="deletion, illegible"/>
                        </del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">resistless</add>
                        </del>
                     </rdg>
                  </subst> currents! 
<!--I CHOSE TO ENCODE "SUCH WRECKS!" AS A REPLACEMENT FOR THE ENTIRE DELETED PHRASE BELOW IT, BUT THE TWO WORD "REPLACEMENTS" WITHIN THAT DELETED PHRASE COULD NOT BE ENCODED AS SEPARATE <RDG>S BECAUSE THERE WOULD OTHERWISE BE A PROBLEM WITH NESTING <subst>S.-->
<subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">The<add rend="unmarked" place="supralinear">
                              <gap reason="deletion, illegible"/>
                           </add> throttled throats and <del rend="overstrike">laps!</del>
                           <add rend="unmarked" place="supralinear">breaths!</add>
                        </del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">such wrecks!</add>
                     </rdg>
                  </subst>
               <lb/>
            </l>

            <l>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">What</del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">The</add>
                        </del>
                     </rdg>
                     <rdg varSeq="3">
                        <add rend="unmarked" place="supralinear">Such</add>
                     </rdg>
                  </subst> unhelm'd ships and all their<lb/>
               freight! <add rend="unmarked" place="interlinear">Such failures—aspiration</add> The<lb/>
            </l>

            <l>
               
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">The</del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">by us</add>
                        </del>
                     </rdg>
                  </subst> 
                  <del rend="overstrike">lives</del> 
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">and out</del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">yearning</add>
                        </del>
                     </rdg>
                  </subst> 
                  <del rend="overstrike">lives</del> 
                  <add rend="insertion" place="supralinear">
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">and loves</del>
                        </rdg>
                        <rdg varSeq="2">
                           <add rend="unmarked" place="supralinear">—the</add>
                        </rdg>
                        <rdg varSeq="3">
                           <add rend="unmarked" place="supralinear">such<add rend="insertion">yearning</add> grand designing</add>
                        </rdg>
                     </subst> 
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">through</del>
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add rend="unmarked" place="supralinear">and</add>
                           </del>
                        </rdg>
                        <rdg varSeq="3">
                           <del rend="overstrike">
                              <add rend="unmarked" place="supralinear">by</add>
                           </del>
                        </rdg>
                     </subst> 
                     <del rend="overstrike">you</del>
                  </add> 
                  <del rend="overstrike">by ye swept</del>
               <lb/>
               
                  <del rend="overstrike">back!</del>
                  <add rend="unmarked" place="interlinear">Such yearning grand designs</add>
               <lb/>
            </l>

            <l>
               <del rend="overstrike">The wrecks invisible</del>
               <add rend="unmarked" place="supralinear">
                  <subst>
                     <rdg varSeq="1">but give</rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">by you</add>
                     </rdg>
                  </subst>
               </add>
               <del rend="overstrike">
                  <add rend="unmarked" place="inline">ye</add>
               </del>
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">carry</del>
                  </rdg>
                  <rdg varSeq="2">
                     <del rend="overstrike">
                        <add rend="unmarked" place="supralinear">b<gap reason="deletion, illegible"/>
                        </add>
                     </del>
                  </rdg>
                  <rdg varSeq="3">
                     <del rend="overstrike">
                        <add rend="unmarked" place="supralinear">under down</add>
                     </del>
                  </rdg>
               </subst>
               <!--FOR THE FOURTH READING, I'M ENCODING INLINE "UNDER" WITH "AT LAST"(DELETED) AND "SWEPT" INSERTED ABOVE IT.-->
               <subst>
                  <rdg varSeq="4">
                     <del rend="overstrike">
                        <add rend="unmarked" place="supralinear">at last</add>
                     </del>
                     <add rend="unmarked" place="supralinear">swept</add>
under</rdg>
               </subst>
            </l>

            <transpose anchored="yes" target="t1">
               <delSpan rend="overstrike"
                        to="d1"
                        />
               <l>
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">To</del>
                     </rdg>
                     <rdg varSeq="2">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">Qu</add>
                        </del>
                     </rdg>
                     <rdg varSeq="3">
                        <del rend="overstrike">
                           <add rend="unmarked" place="supralinear">To</add>
                        </del>
                     </rdg>
                     <rdg varSeq="4">
                        <add rend="unmarked" place="supralinear">Qu</add>
                     </rdg>
                  </subst>
waning light and the <subst>
                     <rdg varSeq="1">
                        <del rend="overstrike">tide</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">stream</add>
                     </rdg>
                  </subst> 
                  <add rend="unmarked" place="infralinear">fast</add> outrunning</l>
               <anchor xml:id="d1"/>
            </transpose>


         </lg>
      </body>
   </text>
