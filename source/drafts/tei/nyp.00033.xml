   <text>
      <body>


         <pb facs="nyp.00033.001" xml:id="leaf001r" type="recto"/>

         <lg type="poem notes"
              >


            <head type="main-derived">
Sea Winrows
</head>

            <l>Sea Winrows</l>
            <l>Ocean Winrows</l>
            <add rend="unmarked" place="interlinear" rend="right">
               <l>Beach</l>
            </add>
            <l>Beach Winrows</l>

            <span value="circled" xml:id="s1" from="s1" to="s2" TEIform="span"/>
            <l>
               Winrows<lb/>
               <anchor xml:id="s2"/>

               with sand and<lb/>
               sea‑hay.—<lb/>
            </l>

            <l>
               Winrows ¶ sand and<lb/>
               <seg rend="indented2">
                  <subst>
                     <rdg varSeq="1">
                        <del rend="overwrite">be</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="overwrite" place="over">scales</add>
                     </rdg>
                  </subst> and<lb/>
               <seg rend="indented3">beach‑hay.<lb/>
            </l>

            <l>Sands and Drifts</l>
            <l>Winrows and Beach‑hay</l>
            <l>Under‑foot</l>

            <milestone unit="undeclared" rend="horbar-short-left"/>
            <l>Walking the Beach</l>
            <milestone unit="undeclared" rend="horbar-short-left"/>
            <l>
               <subst>
                  <rdg varSeq="1">
                     <hi rend="underline">Drift Underfoot</hi>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="right">Underfoot Drift</add>
                  </rdg>
               </subst>
            </l>
            <l>Wash</l>
            <l>Drift at your feet</l>
            <!--THE LAST LINE MAY BE A THIRD ALTERNATE READING FOR "UNDERFOOT DRIFT"-->


         </lg>
      </body>
