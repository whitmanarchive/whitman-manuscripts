      <body>

         <pb facs="loc.00307.001.jpg" xml:id="leaf001r" type="recto"/>

         <lg type="poem">

            <head type="main-authorial">
               <app>
                  <rdg varSeq="1">
                     <del rend="overstrike">Only some</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">
                        <note type="authorial"
                              place="supralinear">?</note>some</add>
                  </rdg>
               </app> Hours 
<del rend="overstrike">&amp;Joys</del> of a half Paralytic</head>

            <delSpan type="hashmark" spanTo="#d1"/>

            <head type="sub">
Only the <app>
                  <rdg varSeq="1">
                     <del rend="overwrite">
                        <gap reason="deletion, illegible"/>
                     </del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="overwrite" place="over">D</add>
                  </rdg>
               </app>aylight
</head>

            <lg type="linegroup">
               <l>Sane, simple, easy hours</l>
               <l>Away from Art—Away</l>
               <l>
                  <del rend="overstrike">Away</del> from Books</l>
               <anchor xml:id="d1"/>
               <del rend="overstrike">
                  <l>Specimen days <unclear reason="deletion, illegible"
                              resp="#fl"
                              cert="medium">of a half-</unclear>
                  </l>
               </del>
               <del rend="overstrike">
                  <l>Only <unclear reason="deletion, illegible"
                              resp="#fl"
                              cert="medium">some story of a half-Paralytic</unclear>
                  </l>
               </del>

               <l>Only <add rend="unmarked" place="interlinear">day &amp; night—only the <app>
                        <rdg varSeq="1">
                           <del rend="overstrike">day</del>
                        </rdg>
                        <rdg varSeq="2">
                           <add rend="unmarked" place="supralinear">Sun</add>
                        </rdg>
                     </app> 
                     <del rend="overstrike">and</del> 
                     <add rend="unmarked" place="supralinear">stars <del rend="overstrike">by</del>
                     </add> 
                     <del rend="overstrike">
                        <gap reason="deletion, illegible"/>
                     </del>
                  </add>  
                  <del rend="overstrike">the light</del> and</l>

               <l>
                  Only the air and the water<lb/>
                  &amp;the grass<lb/>
               </l>
            </lg>

            <lg type="linegroup">
               <l>Sane, simple, easy <app>
                     <rdg varSeq="1">
                        <del rend="overstrike">
                           <gap reason="deletion, illegible"/>
                        </del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="supralinear">hours</add>
                     </rdg>
                  </app>
               </l>
            </lg>

            <lg type="linegroup">
               <l>After the flush <app>
                     <rdg varSeq="1">
                        <del rend="overstrike">the</del>
                     </rdg>
                     <rdg varSeq="2">
                        <add rend="unmarked" place="inline">an</add>
                     </rdg>
                  </app>
               </l>
               <l>
                  
                     <del rend="overstrike">Are they my</del> Indian<lb/>
                  summer <app>
                        <rdg varSeq="1">
                           <del rend="overstrike">how</del>
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add rend="unmarked" place="inline">of</add>
                           </del>
                        </rdg>
                        <rdg varSeq="3">
                           <add rend="unmarked" place="supralinear">to</add>
                        </rdg>
                     </app> my life.<lb/>
               </l>
            </lg>

         </lg>
