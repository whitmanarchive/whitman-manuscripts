            <pb xml:id="leaf001r" facs="loc.01770.001_large.jpg" type="recto"/>
            
            <head type="main-derived">Names</head>
            
            <ab>Names&#8212;in future writing about the War</ab>
            
            <ab>for one side} Nationals</ab>
            
            <ab>the other} Secesh</ab>
            
            <pb xml:id="leaf001v" facs="loc.01770.002_large.jpg" type="verso"/> 
            
            <del rend="hashmark">
                <ab>Mrs. A. <unclear reason="illegible" cert="high">H.</unclear> Price 331 East 55th st. N.Y. City</ab>
                <ab> 
                    <subst>
                    <del rend="overstrike" seq="1">W.W. has</del>
                    <add place="supralinear" rend="unmarked" seq="2">I have</add>
                    </subst>
                    had a <add place="supralinear" rend="insertion">very</add> bad spell, but 
                    <subst>
                        <del rend="overstrike" seq="1">is</del>
                        <add place="supralinear" rend="unmarked" seq="2">am</add>
                    </subst>
                    now
                    <subst>
                        <del rend="overstrike" seq="1">as well</del>
                        <add place="supralinear" rend="unmarked" seq="2">about</add>
                    </subst>
                    <del rend="overstrike">the same</del> as before.  <del rend="overstrike">He is out to-day</del> Recovery yet probable, &#38; a visit to you. 
                </ab>
                
                <ab>
                    W. W. 431 Stevens st.
                    <subst>
                        <del rend="overstrike" seq="1">Camd</del>
                        <add place="supralinear" rend="unmarked" seq="2">Camden</add>
                    </subst>
                    N.J.
                </ab>
            </del>
            
