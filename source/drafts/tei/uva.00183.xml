         <div1 type="multiple poems">
		<!-- AGAIN, UNIQUE ID PLUS 3-DIGIT PAGE # IN THE FOLLOWING -->
            <pb facs="uva.00183.001" xml:id="leaf001r" type="recto"/>

            <lg
                 >

               <head type="main-derived">Once I passed through a populous</head>

               <note place="top"
                     type="authorial">p.311</note>
               <note place="top"
                     type="authorial"
                     rend="circled">9</note>

               <l>
                  Once I <del rend="overstrike">pass'd</del> 
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">passed through</del>
                        </rdg>

                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add place="supralinear" rend="unmarked">remained awhile</add>
                           </del> 
                        </rdg>
                        <rdg varSeq="3">
                           <add place="supralinear" rend="unmarked">passed through a populous</add> 
                        </rdg>
                     </subst>
a<lb/>
                  
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">great</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add place="supralinear" rend="unmarked">populous</add>
                           </del> 
                        </rdg>
                        <rdg varSeq="3">
                           <add place="infralinear" rend="unmarked">celebrated</add> 
                        </rdg>
                     </subst>
city <add place="supralinear" rend="unmarked">imprinting on my brain for <del rend="overstrike">the</del> future use,</add> 
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">full of</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add place="supralinear" rend="unmarked">studying its</add>
                           </del> 
                        </rdg>
                     </subst>
                     <add place="supralinear" rend="unmarked">its</add> shows,<lb/>
                  
                     <add place="left" rend="unmarked">with <del rend="overstrike">all</del>
                     </add> 
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">wealth</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add place="supralinear" rend="unmarked">its shows</add>
                           </del> 
                        </rdg>
                     </subst>
                     <del rend="overstrike">
                        <add place="supralinear" rend="unmarked">notions</add>
                     </del> architecture<add place="inline" rend="unmarked">,</add> 
                     <add place="supralinear" rend="insertion">customs</add> and<lb/>
                  traditions<lb/>
               </l>

               <l>
                  But <delSpan spanTo="#d1"
                              rend="overstrike"
                              />now <add place="supralinear" rend="unmarked">now</add> 
                     <del rend="overstrike">I remember</del> all that<lb/>
                  
                     <del rend="overstrike">po</del> 
                     <del rend="overstrike">city</del> city hasâ€”now I<lb/>
                  remember nothing<anchor xml:id="d1"/> of all<lb/>
                  that city I remember<lb/>
                  
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">nothing except</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <add place="supralinear" rend="unmarked">only the <del rend="overstrike">a</del> man</add> 
                        </rdg>
                     </subst>
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">that there</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <del rend="overstrike">
                              <add place="supralinear" rend="unmarked">how one</add>
                           </del> 
                        </rdg>
                     </subst>
                  <lb/>
                  
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">I wandered</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <add place="supralinear" rend="unmarked">who</add> 
                        </rdg>
                     </subst>
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">day by day,</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <add place="supralinear" rend="unmarked">there</add> 
                        </rdg>
                     </subst>
                     <delSpan spanTo="#d2"
                              rend="overstrike"
                              />with<lb/>
                  one who loved me,<anchor xml:id="d2"/> 
                     <add place="supralinear" rend="unmarked">wandered with me, <add place="supralinear" rend="insertion">there</add> for love of me,</add>
                  <lb/>
               </l>

               <add place="interlinear" rend="unmarked">
                  <l>
                     <del rend="overstrike">We <gap reason="deletion,illegible"/>
                     </del> Day by day, and night by night, we were together
</l>
               </add>

               <l>
                  All else has long been forgotten<lb/>
                  by meâ€”<del rend="overstrike">But</del> I remember<add place="inline" rend="unmarked">,</add> 
                     <add place="supralinear" rend="insertion">I say</add> 
                     <del rend="overstrike">well</del> 
                     <add place="supralinear" rend="unmarked">only</add>
                  <lb/>
                  
                     <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">that youth</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <add place="supralinear" rend="unmarked">one rude and ignorant</add> 
                        </rdg>
                     </subst>
man who, when I<lb/>
                  departed, <add place="supralinear" rend="insertion">lay and lay</add> held me <del rend="overstrike">long</del> by the<lb/>
                  hand, with silent lip, <del rend="overstrike">pale</del> 
                     <add place="inline" rend="unmarked">
                        <unclear reason="illegible" cert="30%" resp="#cf">sad</unclear>
                     </add>
                  <lb/>
                  and tremulous.â€”<lb/>
               </l>
            </lg>

            <pb facs="uva.00183.002" xml:id="leaf001v" type="verso"/>
            <lg
                 >
               <delSpan spanTo="#d3" rend="hashmark" />

               <lg type="linegroup"
                    >
                  <l>I am the child of Democracy,</l>

                  <l>
                     I <del rend="overstrike">I</del> will advance from the<lb/>
                     people enâ€‘masse, in their<lb/>
                     own spirit,<lb/>
                  </l>

                  <l>
                     I will chant <subst>
                           <rdg varSeq="1">
                              <del rend="overstrike">your own</del> 
                           </rdg>
                           <rdg varSeq="2">
                              <add place="supralinear" rend="unmarked">a</add> 
                           </rdg>
                        </subst>
                     <lb/>
                     strong and haughty chant,<lb/>
                     
                        <add place="supralinear" rend="insertion">for you,</add> my mother.â€”<lb/>
                  </l>
               </lg>

               <lg type="linegroup"
                    >
                  <l>I <del rend="overstrike">will make</del>
                  </l>
               </lg>
               <anchor xml:id="d3"/>
            </lg>

            <!-- THESE LINES ARE UPSIDE WITH RESPECT TO THE OTHER VERSO LINES. -->
            <lg
                 >
               <del rend="overstrike">
                  <l>Standing</l>
               </del>

               <del rend="overstrike">
                  <l>Even con</l>
               </del>

               <l>
                  (Though it was thought I was<lb/>
                  baffled and destroyedâ€”but<lb/>
                  that <subst>
                        <rdg varSeq="1">
                           <del rend="overstrike">shall</del> 
                        </rdg>
                        <rdg varSeq="2">
                           <add place="supralinear" rend="unmarked">will</add> 
                        </rdg>
                     </subst>
never be,)<lb/>
               </l>
            </lg>

         </div1>
