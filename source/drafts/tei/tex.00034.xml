         <lg
              >

            <head type="main-derived" rend="bracketed">Light and the senses abdicate</head>

            <l>
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">Thy</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">Light &amp; the</add>
                  </rdg>
               </subst>
 senses abdicate, <del rend="overstrike">O</del>
            </l>
            <l>
               <del rend="overstrike">O</del> Thou <add rend="insertion" place="supralinear">now</add> who to darkness 
<subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">
                        <gap reason="deletion, illegible"/>
                     </del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="infralinear">
                        <del rend="overstrike">retiring</del>
                     </add>
                  </rdg>
                  <rdg varSeq="3">
                     <add rend="insertion" place="supralinear">retiring in</add>
                  </rdg>
               </subst>
thy self brooding
</l>
            <l>
               <subst>
                  <rdg varSeq="1">
                     <del rend="overstrike">Broodest</del>
                  </rdg>
                  <rdg varSeq="2">
                     <add rend="unmarked" place="supralinear">Lookest out</add>
                  </rdg>
               </subst>
 thence <add rend="insertion" place="supralinear">aloof</add> on the world, on thy <add rend="insertion" place="supralinear">comrades</add>
            </l>
            <l>
               <del rend="overstrike">On On thy comrades &amp; on thyself.</del>
            </l>
            <l>
               And on thy own life, passions,<lb/>
               and body.<lb/>
            </l>


         </lg>
